<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>Project</display-name>

	<welcome-file-list>
		<welcome-file>myproject.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>greetServlet</servlet-name>
		<servlet-class>com.fhoster.test.server.GreetingServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>greetServlet</servlet-name>
		<url-pattern>/project/greet</url-pattern>
	</servlet-mapping>

	<servlet>
    	<servlet-name>LivetableTestServlet</servlet-name>
    	<servlet-class>com.fhoster.test.server._LivetableServlet</servlet-class>
  	</servlet>

  <servlet-mapping>
    <servlet-name>LivetableTestServlet</servlet-name>
    <url-pattern>/project/LTServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
